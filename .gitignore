# ===========================================
# Web Final Project - .gitignore
# ===========================================

# ===========================================
# Environment & Secrets (CRITICAL!)
# ===========================================
.env
.env.local
.env.*.local
*.env

# ===========================================
# Dependencies
# ===========================================
/vendor/
/node_modules/
composer.lock
package-lock.json

# ===========================================
# IDE & Editor Settings
# ===========================================
.vscode/
.idea/
*.sublime-project
*.sublime-workspace
*.swp
*.swo
*~

# ===========================================
# Operating System Files
# ===========================================
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
desktop.ini

# ===========================================
# Logs & Cache
# ===========================================
*.log
/logs/
/cache/
/tmp/
*.cache

# ===========================================
# Build & Compiled Files
# ===========================================
/build/
/dist/
*.min.js
*.min.css

# ===========================================
# Database Files (Local only)
# ===========================================
*.sql.bak
*.sqlite
*.db

# ===========================================
# Uploads (if storing locally)
# ===========================================
/uploads/*
!/uploads/.gitkeep

# ===========================================
# Testing
# ===========================================
/coverage/
.phpunit.result.cache
phpunit.xml

# ===========================================
# Miscellaneous
# ===========================================
*.bak
*.backup
*.tmp
*.temp
